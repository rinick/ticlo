    "**/postcss": "^8.4.5"
  },
  "scripts": {
    "test-nyc-strict": "cross-env NODE_ENV=TEST TS_NODE_PROJECT='tsconfig.node.json' nyc --skip-full --check-coverage=true mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts' && nyc report --reporter=text-lcov > coverage/nyc-strict.lcov",
    "test-nyc": "cross-env TS_NODE_PROJECT='tsconfig.node.json' nyc --skip-full --check-coverage=true mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts' && nyc report --reporter=text-lcov",
    "ticlo-test-nyc": "nyc --silent --check-coverage=false ts-node  -P tsconfig.node.json ./bin/test-server.ts --run --quitOnFinish && nyc report --reporter=text-lcov > coverage/nyc-ticlo.lcov",
    "test-karma": "karma start",
    "build-less": "lessc --js src/editor/style/antd.less dist/antd.css && lessc --js src/editor/style/editor.less dist/editor.css",
    "build-icons": "ts-node  -P tsconfig.node.json tool/build-icons",
    "build-i18n": "ts-node  -P tsconfig.node.json tool/merge-lng",
    "build-package": "ts-node  -P tsconfig.node.json tool/build-package.ts",
    "test": "cross-env NODE_ENV=TEST mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts'",
    "express-server": "ts-node  -P tsconfig.node.json example/server/express-server",
    "ticlo-test-server": "ts-node  -P tsconfig.node.json bin/test-server --serve --run",
    "editor-browser": "parcel serve example/simple-editor/editor-browser.html --open --dist-dir temp --no-source-maps --no-hmr",
    "server-and-editor-browser": "parcel serve example/simple-editor/server-and-editor-browser.html --open --dist-dir temp --no-source-maps --no-hmr",
    "browser-server": "parcel serve example/simple-editor/server.html example/simple-editor/editor-browser.html --open --dist-dir temp --no-source-maps",
    "vite-dev": "vite",
    "prettier-check": "prettier --check \"(src|tool|example)/**/*.+(ts|tsx|less)\"",
    "prettier-write": "prettier --write \"(src|tool|example)/**/*.+(ts|tsx|less)\""
  }
}
